{
  "name": "codex-gemini-mcp-sample",
  "version": "0.1.0",
  "description": "Minimal TypeScript MCP server that proxies prompts to Codex and Gemini CLI",
  "type": "module",
  "main": "dist/mcp/codex-standalone-server.js",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "start": "node dist/mcp/codex-standalone-server.js",
    "start:codex": "node dist/mcp/codex-standalone-server.js",
    "start:gemini": "node dist/mcp/gemini-standalone-server.js",
    "dev": "tsx src/mcp/codex-standalone-server.ts",
    "dev:codex": "tsx src/mcp/codex-standalone-server.ts",
    "dev:gemini": "tsx src/mcp/gemini-standalone-server.ts",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "keywords": [
    "mcp",
    "typescript",
    "codex",
    "gemini"
  ],
  "engines": {
    "node": ">=20"
  },
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.27.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/node": "^25.3.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  }
}
